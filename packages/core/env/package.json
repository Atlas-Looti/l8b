{
	"name": "@l8b/env",
	"version": "1.0.0",
	"description": "Environment variables API for LootiScript",
	"sideEffects": false,
	"files": [
		"dist/**/*",
		"README.md",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"keywords": [
		"environment",
		"env",
		"variables",
		"config",
		"game-engine"
	],
	"engines": {
		"node": ">=18"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}
